generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id                   Int       @id @unique @default(autoincrement())
  email                String    @unique
  username             String    @unique
  image                String?
  created_at           DateTime  @default(now())
  updated_at           DateTime
  uuid                 String    @unique
  refresh_token        String?
  refresh_token_expiry DateTime?
}
